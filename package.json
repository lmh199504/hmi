{
  "name": "account",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "build:prod": "cross-env UMI_ENV=prod umi build",
    "build:test": "cross-env UMI_ENV=test umi build",
    "build:uat": "cross-env UMI_ENV=uat umi build",
    "commit": "git-cz",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -w -r 0",
    "lint": "umi g tmp && npm run lint:js && npm run lint:style && npm run lint:prettier",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src && npm run lint:style",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:prettier": "prettier -c --write \"src/**/*\" --end-of-line auto",
    "lint:style": "stylelint --fix \"src/**/*.less\" --customSyntax postcss-less",
    "precommit": "lint-staged",
    "svgo": "svgo -f src/icons/svg --config=src/icons/svgo.config.js",
    "analyze": "cross-env ANALYZE=1 umi build"
  },
  "gitHooks": {
    "pre-commit": "lint-staged",
    "commit-msg": "commitlint -E GIT_PARAMS"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "homepage": "/account",
  "dependencies": {
    "@alitajs/keep-alive": "^2.9.0",
    "@ant-design/pro-components": "^2.4.9",
    "@ant-design/pro-table": "^2.57.1",
    "@umijs/preset-react": "1.8.5",
    "ahooks": "^3.7.6",
    "amfe-flexible": "^2.2.1",
    "axios-mapper": "^0.5.6",
    "cross-env": "7.0.3",
    "js-cookie": "2.2.1",
    "postcss-px2rem-exclude": "^0.0.6",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "umi": "3.4.11",
    "umi-plugin-keep-alive": "^0.0.1-beta.35"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^14.1.0",
    "@types/lodash": "^4.14.194",
    "@types/webpack-env": "^1.18.0",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "@umijs/plugin-qiankun": "2.24.1",
    "commitizen": "^4.2.4",
    "commitlint": "^14.1.0",
    "duplicate-package-checker-webpack-plugin": "^3.0.0",
    "eslint": "^8.39.0",
    "eslint-config-alloy": "^4.9.0",
    "eslint-plugin-react": "^7.32.2",
    "lint-staged": "^11.0.1",
    "prettier": "^2.2.0",
    "stylelint": "^14.0.1",
    "stylelint-config-css-modules": "^2.2.0",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.1.0",
    "svg-sprite-loader": "^6.0.11",
    "svgo": "^3.0.2",
    "typescript": "^4.1.2",
    "yorkie": "^2.0.0"
  }
}
